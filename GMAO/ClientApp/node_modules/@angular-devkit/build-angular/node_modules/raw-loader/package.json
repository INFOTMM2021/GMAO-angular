{
  "name": "raw-loader",
  "version": "3.1.0",
  "description": "A loader for webpack that allows importing files as a String",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/raw-loader.git"
  },
  "author": {
    "name": "Tobias Koppers @sokra"
